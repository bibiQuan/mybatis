<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenjianquan.mybatis.dao.EmployeeMapper">
	<select id="getEmployeeById" resultType="com.chenjianquan.mybatis.bean.Employee">
		select id, last_name lastName, gender, email from employees where id = #{id}
	</select>
</mapper>